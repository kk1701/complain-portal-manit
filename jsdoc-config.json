{
    "tags": {
      "allowUnknownTags": true
    },
    "source": {
      "include": ["."],
      "includePattern": ".+\\.js?$",
      "exclude": ["purpose.text", ".env", ".gitignore", "package-lock.json", "package.json"],
      "excludePattern": "(^|\\/|\\\\)node_modules"
    },
    "opts": {
      "destination": "./docs/",
      "recurse": true,
      "template": "node_modules/clean-jsdoc-theme"
    },
    "templates": {
      "default": {
        "includeDate": false
      }
    }
  }